{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug React App",
        "type": "pwa-chrome",
        "request": "launch",
        "url": "http://localhost:5173",
        "webRoot": "${workspaceFolder}/src",
        "sourceMapPathOverrides": {
          "webpack:///*": "${webRoot}/*",
          "webpack:///src/*": "${webRoot}/*",
          "webpack:///./src/*": "${webRoot}/*",
          "webpack:///./node_modules/*": "${webRoot}/node_modules/*"
        },
        "breakOnLoad": true
      }
    ]
  }
  